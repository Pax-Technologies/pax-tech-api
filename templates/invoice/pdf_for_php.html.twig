{#{% extends 'base.html.twig' %}#}

{% block body %}
    <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text><text y=%221.3em%22 x=%220.2em%22 font-size=%2276%22 fill=%22%23fff%22>sf</text></svg>">
<link href="https://fonts.googleapis.com/css2?family=Lato:wght@300;400;700&display=swap" rel="stylesheet">
    <style>

        body {
            font-family: 'Lato', sans-serif; /* Utiliser la police Lato */
        }
        /* Styles par défaut */
        #invoice-details td input {
            width: 100%;
            box-sizing: border-box;
        }

        #invoice-details tr {
            border: transparent;
        }

        .show-on-pdf {
            display: none;
        }

        /* DIVISION PDF */
        .pdf-mode .card {
            background-color: white;
            color: black;
        }

        .pdf-mode #invoice-details {
            background-color: white !important;
            color: black !important;
        }

        .pdf-mode #invoice-details th, .pdf-mode #invoice-details td {
            color: black !important;
        }

        .pdf-mode {
            border-top: 10px solid #EB0045;
            color: black;
            background-color: white;
            font-size: 11px;
        }

        .h5 {
            font-size: 1.5em;
            font-weight: bold;
            padding-bottom: 5px;
        }

        .h6 {
            font-size: 1.1em !important;
        }

        .total {
            margin-top: 15px;
            font-size: 2em;
        }

        .pdf-mode {
            --bs-body-bg: white !important; /* Définir la variable ici */
        }

        .pdf-mode .fade-title {
            color: lightgrey !important;
        }

        .pdf-mode .show-on-pdf {
            display: block !important;
        }

        .container {
            width: 100%;
            margin: auto;
            padding-top: 0;

        }

        .header, .footer {

            margin-bottom: 20px;
        }


        p {
            margin: 5px 0;
        }

        .text-primary {
            color: #00BC70;
        }

        .fade-title {
            opacity: 0.6;
        }

        .show-on-pdf {
            display: block;
        }

        .small {
            font-size: 0.9em;
        }

        table {
            width: 100%;
            border-collapse: collapse;
            margin-top: 0px;
        }

        table th, table td {
            border: none;
            padding: 8px;
            text-align: left;
        }

        table th {

            color: white;
        }

        .card {
            border: 1px solid #ddd;
            border-radius: 4px;
            margin-bottom: 20px;
        }

        .card-header {
            color: black;
            padding: 10px;
            border-bottom: 1px solid #ddd;
        }

        .card-body {
            padding: 10px;
            border-bottom: 1px solid #ddd;
        }

        .card-footer {
            background-color: #f8f9fa;
            padding: 10px;

        }

        .left-column {
            width: 50%;
            vertical-align: top;
        }

        .right-column {
            width: 50%;
            text-align: right;
        }

        .header .left-column {
            width: 70%;
            vertical-align: top;
        }

        .header .right-column {
            width: 30%;
            text-align: right;
            padding-top: 20px;
        }
        .invoice-line {
            display: block;  /* Chaque élément prendra toute la largeur */
            margin-bottom: 3px;  /* Espace entre chaque ligne */
        }

    </style>
</head>

<div class="container invoice-content pdf-mode">
    <div class="header">
        <table>
            <tr>
                <td class="left-column">
                    <h2>Facture n°{{ invoice.invoiceNumber }}</h2>
                    <p>Date: {{ invoice.date|date("d-m-Y") }}</p>
                </td>
                <td class="right-column">
                    <img src="data:image/png;base64," height="auto" width="150" class="show-on-pdf">
                </td>
            </tr>
        </table>
    </div>

    <div class="card">
        <div class="card-header">
            <div class="h6 fade-title">Informations</div>
            <table>
                <tr>
                    <td class="small" style="width: 50%;">
                        <div class="h5">Société</div>
                        <span class="invoice-line"><strong>Nom: </strong>Pax Technologies</span>
                        <span class="invoice-line"><strong>Numéro de TVA: </strong>BE 0771.359.440</span>
                        <span class="invoice-line"><strong>Adresse: </strong>33, 4ème Rue - 6040 Jumet</span>
                        <span class="invoice-line"><strong>Téléphone: </strong>+32 474 65 23 18</span>
                        <span class="invoice-line"><strong>Email: </strong>invoices@pax-tech.com</span>
                    </td>
                    <td class="small" style="width: 50%;">
                        <div class="h5">Client</div>
                        <span class="invoice-line"><strong>Nom: </strong>{{ invoice.client.company }}</span>
                        <span class="invoice-line"><strong>Numéro de TVA: </strong>{{ invoice.client.vATNumber }}</span>
                        <span class="invoice-line"><strong>Adresse: </strong>{{ invoice.client.billingAddress }}</span>
                        <span class="invoice-line"><strong>Téléphone: </strong>{{ invoice.client.phone }}</span>
                        <span class="invoice-line"><strong>Email: </strong>{{ invoice.client.email }}</span>
                    </td>
                </tr>
            </table>
        </div>
        <div class="card-body">
            <div class="h6 fade-title">Détail</div>
            <table id="invoice-details">
                <thead>
                <tr>
                    <th>Description</th>
                    <th>Prix unitaire</th>
                    <th>Quantité</th>
                    <th>Total HTVA</th>
                    <th>TVA</th>
                    <th>Total TVAC</th>
                </tr>
                </thead>
                <tbody>
                {% for detailForm in invoice.invoiceDetails %}
                    <tr>
                        <td>{{ detailForm.description }}</td>
                        <td>{{ detailForm.unitPriceExcl|number_format(2, ',', ' ') }} €</td>
                        <td>{{ detailForm.quantity }}</td>
                        <td>{{ detailForm.totalExcl|number_format(2, ',', ' ') }} €</td>
                        <td>{{ detailForm.vatRate }} %</td>
                        <td>{{ detailForm.totalIncl|number_format(2, ',', ' ') }} €</td>
                    </tr>
                {% endfor %}
                </tbody>
            </table>
        </div>
        <div class="card-footer">
            <div class="h6 fade-title pt-1">Informations financières</div>
                <table style="width: 100%;">
                    <tr>
                        <td class="left-column">
                            <div class="show-on-pdf">
                                Veuillez payer cette facture sur le numéro de compte
                                <p class="text-primary"><strong>BE16 9734 9196 8674</strong></p>
                                avec la communication
                                <p class="text-primary"><strong>{{ invoice.invoiceNumber }}</strong></p>
                                Date d'échéance de la facture :
                                <p class="text-primary">
                                    <strong>{{ "now"|date_modify("+30 days")|date("d-m-Y") }}</strong>
                                </p>
                            </div>
                        </td>
                        <td class="right-column">
                            <div class=h6">Total HTVA: <strong>{{ invoice.totalAmountNet|number_format(2, '.', ',') }} €</strong></div>
                            <div class=h6">Total TVA: <strong>{{ invoice.totalVatAmount|number_format(2, '.', ',') }} €</strong></div>
                            <div class="total">Total TVAC: <strong>{{ invoice.totalAmountGross|number_format(2, '.', ',') }} €</strong></div>
                        </td>
                    </tr>
                </table>
        </div>

    </div>
    Les conditions générales de vente s'appliquent.
</div>

    <script>
        document.getElementById('download-pdf').addEventListener('click', function () {
            // Sélectionne l'élément HTML que tu veux convertir en PDF
            const invoiceElement = document.querySelector('.invoice-content');
            const invoice = {
                invoiceNumber: '{{ invoice.invoiceNumber }}',
                client: {
                    company: '{{ invoice.client.company }}'
                }
            };

            // Ajouter une classe pour les styles PDF
            invoiceElement.classList.add('pdf-mode');

            // Options pour html2pdf
            var opt = {
                margin: [20, 10, 20, 10], // Marges: [haut, droite, bas, gauche]
                filename: `facture_${invoice.invoiceNumber}-${invoice.client.company}.pdf`, // Nom de fichier
                image: {type: 'jpeg', quality: 1}, // Format d'image et qualité
                html2canvas: {
                    dpi: 192, // Résolution
                    scale: 4, // Échelle pour améliorer la qualité
                    letterRendering: true, // Amélioration du rendu des lettres
                    useCORS: true // Utiliser CORS pour les images externes
                },
                jsPDF: {unit: 'mm', format: 'a4', orientation: 'portrait'} // Configurations jsPDF
            };

            // Générer le PDF
            html2pdf().from(invoiceElement).set(opt).save().then(() => {
                // Retirer la classe pour revenir au style normal après le téléchargement
                invoiceElement.classList.remove('pdf-mode');
            });
        });
        // Gestionnaire pour le switch
        document.getElementById('toggle-pdf-mode').addEventListener('change', function () {
            const invoiceElement = document.querySelector('.invoice-content');
            invoiceElement.classList.toggle('pdf-mode', this.checked); // Ajouter ou retirer la classe pdf-mode en fonction de l'état du switch
        });
    </script>

{% endblock %}

